{
  "name": "react-causality-redux",
  "version": "2.0.20",
  "description": "Causality based extension of react-redux",
  "main": "lib/react-causality-redux.js",
  "scripts": {
    "test": "mocha --reporter spec --require babel-register --require ./test/test.js ./test/*-test.js",
    "debugtest": "mocha --debug-brk --inspect ./test/test.js",
    "build": "npm run build:es && npm run build:min",
    "build:es": "cross-env NODE_ENV=none webpack --config webpack.config.js",
    "build:min": "cross-env NODE_ENV=min webpack --config webpack.config.js"
  },
  "author": {
    "name": "Andrew Banks",
    "email": "andrewbanks10@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.1",
    "jsdom": "^11.0.0",
    "mocha": "^4.0.1",
    "webpack": "^3.0.0",
    "webpack-closure-compiler": "^2.1.4",
    "lodash": "^4.17.4"
  },
  "dependencies": {
    "redux": "^3.7.1",
    "react-redux": "^5.0.5",
    "causality-redux": "^2.1.23"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewBanks10/react-causality-redux"
  }
}
